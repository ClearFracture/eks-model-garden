{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": [
				"bedrock:InvokeModel",
				"bedrock:ListFoundationModels"
			],
			"Resource": [
				"arn:aws:bedrock:us-east-1::foundation-model/*",
				"arn:aws:bedrock:us-east-2::foundation-model/*"
			]
		},
		{
			"Effect": "Allow",
			"Action": "bedrock:ListFoundationModels",
			"Resource": "*"
		}
	]
} 